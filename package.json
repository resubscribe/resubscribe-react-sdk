  "scripts": {
    "build": "tsup"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "color": "^4.2.3",
    "goober": "^2.1.10",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/react": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^6.7.0",
    "typescript": "^5.4.5"
  }
